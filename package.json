{
  "name": "modern-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start-server",
    "start-server": "node server.js",
    "build-start": "npm run build-aot && npm run start-server",
    "start-dev": "ng serve --port 7600",
    "sd": "ng serve --port 7600",
    "start-dev-proxy": "ng serve --proxy-config proxy.config.json --port 7600",
    "start-dev-open": "ng serve --open --port 7600",
    "start-dev-proxy-open": "ng serve --open --proxy-config proxy.config.json --port 7600",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "ng build",
    "build-modern-dev": "ng build modern --configuration=dev",
    "build-modern-production": "ng build modern --configuration=production",
    "build-deliverme-dev": "ng build deliverme --configuration=dev",
    "build-deliverme-production": "ng build deliverme --configuration=production",
    "build-carmaster-dev": "ng build carmaster --configuration=dev",
    "build-carmaster-production": "ng build carmaster --configuration=production",
    "serve-modern": "ng serve --project=modern --port=5200",
    "serve-modern-public": "ng serve --project=modern --port=5200 --host 0.0.0.0",
    "serve-deliverme": "ng serve --project=deliverme --port=4200",
    "serve-carmaster": "ng serve --project=carmaster --port=6200",
    "deploy-cloudfront-dev": "node deploy-cloudfront-all.js dev",
    "deploy-cloudfront-production": "node deploy-cloudfront-all.js production",
    "deploy-cloudfront-modern-dev": "node deploy-cloudfront.js modern dev",
    "deploy-cloudfront-modern": "node deploy-cloudfront.js modern production && node deploy-cloudfront.js modern dev",
    "deploy-cloudfront-modern-production": "node deploy-cloudfront.js modern production",
    "deploy-cloudfront-deliverme-dev": "node deploy-cloudfront.js deliverme dev",
    "deploy-cloudfront-deliverme-production": "node deploy-cloudfront.js deliverme production",
    "deploy-cloudfront-carmaster-dev": "node deploy-cloudfront.js carmaster dev",
    "deploy-cloudfront-carmaster-production": "node deploy-cloudfront.js carmaster production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
